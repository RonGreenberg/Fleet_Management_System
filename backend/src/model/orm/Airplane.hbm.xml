<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.orm.Airplane" table="airplanes">
        <id name="planeID" type="java.lang.String" column="planeID" length="7">
            <generator class="assigned"/>
        </id>
        <property name="model" type="java.lang.String" column="model" />
        <property name="dateAdded" type="java.sql.Date" column="dateAdded" />
        <property name="lastPosition" type="java.lang.String" column="lastPosition" />
        <property name="lastHeading" type="java.lang.Double" column="lastHeading" />
        <property name="lastAltitude" type="java.lang.Double" column="lastAltitude" />

    </class>
</hibernate-mapping>
